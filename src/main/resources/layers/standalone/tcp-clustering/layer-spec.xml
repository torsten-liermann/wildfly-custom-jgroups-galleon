<?xml version="1.0" encoding="UTF-8"?>
<layer-spec xmlns="urn:jboss:galleon:layer-spec:1.0" name="tcp-clustering">
    <!-- Layer for TCP-only clustering. Should provide clustering capabilities with only TCP stack (no UDP). -->
    
    <dependencies>
        <layer name="cloud-server"/>
        <layer name="transactions"/>
    </dependencies>
    
    <feature-group name="tcp-only-stack"/>
    
</layer-spec>